<div id="yui-main">
<div class="yui-b main_column">
	<% rounded(:class => "tips") do %>
<h1>Editing space</h1>

<%= error_messages_for :space %>
<% form_for :space, :url => space_path(@space.id), :html => {:method => :put} do |f| %>

<%= render :partial => 'form', :locals => { :f => f } %>
<p>
<%= submit_tag 'Update' %>
 <%= link_to 'Back', spaces_path%>
</p>
<% end %>
<% end %>
</div>
</div>
<%= render :partial =>'shared/sidebar'%>