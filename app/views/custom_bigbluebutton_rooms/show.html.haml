= render :partial => 'manage/menu'

#manage
  = simple_form_for @room, :url => url do |f|
    = f.input :name, :required => false
    = f.input :meetingid, :required => false
    = f.input :server_id, :required => false
    = f.input :randomize_meetingid, :required => false, :as => :boolean
    = f.input :private, :required => false, :as => :boolean
    = f.input :attendee_password, :required => false
    = f.input :moderator_password, :required => false
    = f.input :welcome_msg, :required => false
    = f.input :logout_url, :required => false
    = f.input :dial_number, :required => false
    = f.input :voice_bridge, :required => false
    = f.input :max_participants, :required => false
    = f.input :external, :required => false, :as => :boolean
    = f.input :param, :required => false

    %br%br
    = link_to t('edit'), edit_bigbluebutton_room_path(@room), :class => 'btn'
    = link_to t('join'), join_bigbluebutton_room_path(@room), :class => 'btn'
    = link_to t('bigbluebutton_rails.rooms.invite'), invite_bigbluebutton_room_path(@room), :class => 'btn'
    = link_to t('bigbluebutton_rails.rooms.join_mobile'), join_mobile_bigbluebutton_room_path(@room), :class => 'btn'
    = link_to t('end'), end_bigbluebutton_room_path(@room), :confirm => t('are_you_sure'), :class => 'btn'
    = link_to t('destroy'), bigbluebutton_room_path(@room), :confirm => t('are_you_sure'), :method => :delete, :class => 'btn'
    %br%br
    = link_to t('back'), bigbluebutton_rooms_path, :class => 'btn'
