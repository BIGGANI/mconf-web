<h1>New reader</h1>

<% form_for(:reader, @reader, :url => space_readers_path(@space)) do |f| %>
  <%= f.error_messages %>
	<%= f.text_field :url%>
  <p>
  	<%= f.hidden_field :space_id, :value => @space.id %>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', space_readers_path(@space) %>

<%= render :partial =>'shared/sidebar_users'%>
