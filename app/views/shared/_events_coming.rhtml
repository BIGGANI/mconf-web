<%if @events ==  nil %>
<%= "No Coming events" %>
<%else %>
	<%if @events.size != 0%>
			<!--genera la tabla de eventos-->
       		<%=generate_event_table%>	
			<% @events.each do |event| %>
				<div class="summary">
    				<%=show_event(event)%> <!--muestra cada uno de los eventos-->
				</div>
			<%end %>
	<%end %>
<%end %>