<% @space.container_entries.first(5).each do |entry| -%>
  <p>
    <%= link_to sanitize(entry.title), space_entry_path(@space, entry) %>
    <%= entry.content.class %>
  </p>
<% end -%>
<% if @space.id == 1%>
<% @public_entries.each do |entry|%>
<p>
    <%= link_to sanitize(entry.title), entry_path(entry) %>
    <%= entry.content.class %>
  </p>
<% end -%>
<% end -%>

