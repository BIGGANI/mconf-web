<div class="main_column">
<% rounded(:class => "edit_profile") do %>
<h1>Editing profile</h1>

<%= error_messages_for :profile %>

<% form_for :profile, :url => space_user_profile_path(@space.id, @user.id), :html => {:method => :put} do |f| %>


<%= render :partial => 'form', :locals => { :f => f } %>

        	<%= submit_tag 'Update' %> &nbsp;&nbsp;&nbsp;&nbsp;<input onclick=document.location="/" type="button" value="Back" />
        
<% end %>
<% end %>
</div>
