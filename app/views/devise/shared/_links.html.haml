%h3= t('other_options')
%ul
  %li= link_to t('cafe.login'), shibboleth_path
  - if controller_name != 'sessions'
    %li= link_to t('login.one'), login_path
  - if devise_mapping.registerable? && controller_name != 'registrations'
    %li= link_to t('register.one'), register_path
  - if devise_mapping.recoverable? && controller_name != 'passwords'
    %li= link_to t('user.lost_password'), new_password_path(resource_name)
  - if devise_mapping.confirmable? && controller_name != 'confirmations'
    %li= link_to t("user.resend_confirmation_email.link"), new_confirmation_path(resource_name)
