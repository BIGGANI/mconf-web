
<div class="contenido_search">    
  <%if @events == []%>
		<h3>No events found</h3>
	<% elsif @events.size == 1%>
		<h3> 1 event was found</h3>
		<% @events.each do |event| %>  
			<%= render :partial => event %>
		<% end %>
	<%else%>
		<h3><%= @events.size%> events were found</h3>
	  <% @events.each do |event| %>  
      <%= render :partial => event %>
		<% end %>
	<%end%>
</div>
