.user.disabled
  = raw link_logo_image(user, :size => '32', :title => user.name, :url => user_path(user), :class => 'logo logo-user')


  %ul.group_links
    = link_to icon_confirm(:alt => t('enable'), :title => t('user.enable')), enable_user_path(user.id), :method => :post

  .user_data
    %ul
      %li.main_text
        = t('name.one') + ":"
        = user.name
