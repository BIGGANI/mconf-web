<%div_for(event) do%>
  <div class="group_links">
  	Updated <%=time_ago_in_words(event.updated_at)%> ago
    <%=link_to "edit" , space_events_path(@space,:edit => event) %> 
	<%=link_to("destroy" , space_event_path(@space, event),:method => :delete) %><br/>
  </div>
  <h6><%=event.title%></h6> <%=event.start_date.to_formatted_s(:short)%> - <%=event.end_date.to_formatted_s(:short)%>   <br/>
  <%=event.description%> 

  <%end%>