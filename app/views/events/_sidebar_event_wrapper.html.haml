- if events.empty?
  %p= t('event.no_events')
- else
  - for event in events
    .event-wrapper
      = link_to sanitize(event.title), space_event_path(event.space, event), :class => "event-title"
      %span.event-date
        = "#{event.start_date.to_formatted_s(:short)} - #{event.end_date.to_formatted_s(:short)}".html_safe
