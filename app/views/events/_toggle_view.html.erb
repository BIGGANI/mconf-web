  $(document).ready(function(){
    $(".without_javascript").hide();
		$(".javascript").show();
		$("#expanded_date").hide();
		$("#expanded_online").hide();
		$("#expanded_invite").hide();
		
    $("a#toggle_date").click(function () {
      $("div#expanded_date").toggle("slow");
    });
	
	$("a#toggle_online").click(function () {
      $("div#expanded_online").toggle("slow");
    });
	
    $("a#toggle_invite").click(function () {
      $("div#expanded_invite").toggle("slow");
    });

  });
