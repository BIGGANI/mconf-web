- spaces_menu_at :events
= content_for :menu do
  = render :partial => 'spaces/menu'
- atom_link_header(space, Event.new, :title => "#{t('event.other')} - #{space.name}")

#new-event
  = simple_form_for [@space, @event] do |f|
    = render :partial => "form_fields", :locals => { :f => f }
    = f.button :wrapped, :cancel => space_events_path, :value => t("_other.create")
