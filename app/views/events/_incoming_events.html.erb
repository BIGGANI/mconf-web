        <h3>Incoming Events</h3>
        <span id="today_text"<%= 'style="display: none;"' if params[:day] != 'today' %>><%="Today (#{@today_events.size})"%> |</span>
				<span id="today_events_link"<%= 'style="display: none;"' if params[:day] == 'today' %>><%=link_to "Today (#{@today_events.size})" , space_events_path(@space, :show => "incoming_events", :day => "today"),{:title => "Today Events", :id => 'today_link'}%> |</span>
				<span id="next_week_text"<%= 'style="display: none;"' if params[:day] != 'next_week' %>><%="within 7 days (#{@next_week_events.size})"%> |</span>
				<span id="next_week_events_link"<%= 'style="display: none;"' if params[:day] == 'next_week' %>><%=link_to "within 7 days (#{@next_week_events.size})", space_events_path(@space, :show => "incoming_events", :day => "next_week" ),{:title => "the events within 7 days" , :id => 'next_week_link'}%> |</span>
				<span id="next_month_text"<%= 'style="display: none;"' if params[:day] != 'next_month' %>><%="within a Month (#{@next_month_events.size})"%> |</span>
