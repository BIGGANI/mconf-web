				<span id="all_incoming_text"<%= 'style="display: none;"' if (params[:day] == 'today' or params[:day] == 'next_week' or params[:day] == 'next_month' ) %>><%="All Incoming (#{@all_incoming_events.size})"%> </span>
        <span id="all_incoming_events_link"<%= 'style="display: none;"' if (params[:day].nil? or (params[:day] != 'today' and params[:day] != 'next_week' and params[:day] != 'next_month')) %>><%=link_to "All Incoming (#{@all_incoming_events.size})", space_events_path(@space, :show => "incoming_events"),{:title => "All Coming events", :id => 'all_incoming_link'}%> <br/></span> 

