      <p> No today events</p>
    <%else%>
      <%for event in @today_paginate_events%>
        <%= render :partial => event %>
      <%end%>
			<br/><br/> 
		<div id="today_paginate">
    	<%= will_paginate @today_paginate_events%>
    </div>
		<%end%>	    
		</div>
		
		<div id="next_week_events" <%= 'style="display: none;"' if params[:day] != 'next_week'%>>
