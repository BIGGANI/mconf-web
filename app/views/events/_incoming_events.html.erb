        <p> No next month events</p>
    <%else%>
      <%for event in @next_month_paginate_events%>
        <%= render :partial => event %>
      <%end%>
      <br/><br/>
		<div id="next_month_paginate">	 
    	<%= will_paginate @next_month_paginate_events%>
		</div>
    <%end%> 
    </div>
		
		<div id="all_incoming_events"<%= 'style="display: none;"' if (params[:day] == 'today' or params[:day] == 'next_week' or params[:day] == 'next_month')%>>
