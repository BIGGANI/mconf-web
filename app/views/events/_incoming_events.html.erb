    <%else%>
      <%for event in @next_week_paginate_events%>
        <%= render :partial => event %>
      <%end%>
      <br/><br/>
		<div id="next_week_paginate">	 
    	<%= will_paginate @next_week_paginate_events%>
    </div>
		<%end%>    			
    </div>
		
		<div id="next_month_events"<%= 'style="display: none;"' if params[:day] != 'next_month'%>>
