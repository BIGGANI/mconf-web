<div id="events_content">
  <h3>Upcoming Events</h3> 
	  <%if @first_upcoming_events.empty?%>
      <p> No Upcoming events</p>
	  <%else%>
	    <%for event in @first_upcoming_events%>
	      <%= render :partial => event %>
	    <%end%>
			 <div class="group_links">
         <%=link_to "Show all Upcoming Events", space_events_path(@space, :show => "upcoming_events"), {:class => 'upcoming_link'}%>
       </div>	
	  <%end%>
  
  <br />
  <br />
  
  <h3>Past Events</h3>  

	  <%if @last_past_events.empty?%>
	    <p> No past events</p>
	  <%else%>
	    <%for event in @last_past_events%>
	      <%= render :partial => event %>
	    <%end%>
			<div class="group_links">
        <%=link_to "Show all Old Events", space_events_path(@space, :show => "past_events"), {:class => 'past_link'}%> 
      </div>  	
	  <%end%>
  </div>