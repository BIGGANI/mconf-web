$(document).ready(function() {

  $("a#new_private_message_link").fancybox({
    'hideOnContentClick' : false,
    'frameWidth' : 450,
    'frameHeight' : 430
  });

  /*jquery for assistants div*/
  $(".list_my_spaces").livequery('click',function () {
    $("#my_spaces_lite").hide();
    $("#all_my_spaces").hide();
    $("#my_spaces_list").show();
    return false;
  });

   $(".more_spaces").livequery('click',function () {
    $("#my_spaces_lite").hide();
    $("#my_spaces_list").hide();
    $("#all_my_spaces").show();
    return false;
  });

   $(".less_spaces").livequery('click',function () {
     $("#all_my_spaces").hide();
     $("#my_spaces_list").hide();
     $("#my_spaces_lite").show();
    return false;
  });

});
